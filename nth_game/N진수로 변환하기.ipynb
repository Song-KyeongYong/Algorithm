    "def convert(n, base):\n",
    "    H = \"0123456789ABCDEF\"\n",
    "    q, r = divmod(n, base)\n",
    "    if q==0:\n",
    "        return H[r]\n",
    "    else:\n",
    "        return convert(q,base) + H[r]\n",
    "convert(233,16)"
   ]
